      <row xmlns="http://cnx.rice.edu/cnxml">
      <entry><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><msup><mrow><mn>10</mn></mrow><mrow><mo>−</mo><mn>4</mn></mrow></msup></mrow></math></entry>
      <entry xmlns="http://cnx.rice.edu/cnxml"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mfrac><mn>1</mn><mrow><msup><mrow><mn>10</mn></mrow><mn>4</mn></msup></mrow></mfrac><mo>=</mo><mfrac><mn>1</mn><mrow><mn>10</mn><mo>×</mo><mn>10</mn><mo>×</mo><mn>10</mn><mo>×</mo><mn>10</mn></mrow></mfrac><mo>=</mo><mfrac><mn>1</mn><mrow><mn>10,000</mn></mrow></mfrac></mrow></math></entry>
