      <entry xmlns="http://cnx.rice.edu/cnxml"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mn>15.</mn><mo>×</mo><mn>10000</mn><mo>=</mo><msub><mrow><mn>15</mn></mrow><mo>∧</mo></msub><munder accentunder="true"><mn>0</mn><mo>⌣</mo></munder><munder accentunder="true"><mn>0</mn><mo>⌣</mo></munder><munder accentunder="true"><mn>0</mn><mo>⌣</mo></munder><munder accentunder="true"><mn>0</mn><mo>⌣</mo></munder><mo>.</mo><mo>=</mo><mn>150,000</mn></mrow></math></entry>
