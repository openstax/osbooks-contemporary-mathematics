<document xmlns="http://cnx.rice.edu/cnxml">
  <title>Integer Powers of 10</title>
  <metadata xmlns:md="http://cnx.rice.edu/mdml">
    <md:title>Integer Powers of 10</md:title>
    <md:content-id>m00118</md:content-id>
