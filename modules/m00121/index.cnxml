    <row>
    <entry>Complete Graph</entry>
    <entry>Cycle</entry>
    <entry>Cycle</entry>
    <entry>Cycle</entry>
    </row>
    </thead>
    <tbody>
    <row>
    <entry><figure class="unnumbered" id="fig-00008">
    <media alt="Insert Alt Text">
    <image mime-type="image/png" src="../../media/CS_Figure_12_07_195a.png"/>
    </media></figure></entry>
    <entry><figure class="unnumbered" id="fig-00009">
    <media alt="Insert Alt Text">
    <image mime-type="image/png" src="../../media/CS_Figure_12_07_195b.png"/>
    </media></figure></entry>
    <entry><figure class="unnumbered" id="fig-00010">
    <media alt="Insert Alt Text">
    <image mime-type="image/png" src="../../media/CS_Figure_12_07_195c.png"/>
    </media></figure></entry>
    <entry><figure class="unnumbered" id="fig-00011">
    <media alt="Insert Alt Text">
    <image mime-type="image/png" src="../../media/CS_Figure_12_07_195d.png"/>
    </media></figure></entry>
    </row>
    <row>
    <entry><emphasis effect="bold">Cycle Name Clockwise</emphasis></entry>
    <entry><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mrow><mo>(</mo><mrow><mi>a</mi><mo>,</mo><mi>b</mi><mo>,</mo><mi>c</mi><mo>,</mo><mi>d</mi></mrow><mo>)</mo></mrow></mrow></math></entry>
    <entry xmlns="http://cnx.rice.edu/cnxml"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mrow><mo>(</mo><mrow><mi>a</mi><mo>,</mo><mi>b</mi><mo>,</mo><mi>d</mi><mo>,</mo><mi>c</mi></mrow><mo>)</mo></mrow></mrow></math></entry>
    <entry xmlns="http://cnx.rice.edu/cnxml"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mrow><mo>(</mo><mrow><mi>a</mi><mo>,</mo><mi>c</mi><mo>,</mo><mi>b</mi><mo>,</mo><mi>d</mi></mrow><mo>)</mo></mrow></mrow></math></entry>
    </row>
    <row xmlns="http://cnx.rice.edu/cnxml">
    <entry><emphasis effect="bold">Cycle Name Counterclockwise</emphasis></entry>
    <entry><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mrow><mo>(</mo><mrow><mi>a</mi><mo>,</mo><mi>d</mi><mo>,</mo><mi>c</mi><mo>,</mo><mi>b</mi></mrow><mo>)</mo></mrow></mrow></math></entry>
    <entry xmlns="http://cnx.rice.edu/cnxml"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mrow><mo>(</mo><mrow><mi>a</mi><mo>,</mo><mi>c</mi><mo>,</mo><mi>d</mi><mo>,</mo><mi>b</mi></mrow><mo>)</mo></mrow></mrow></math></entry>
    <entry xmlns="http://cnx.rice.edu/cnxml"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mrow><mo>(</mo><mrow><mi>a</mi><mo>,</mo><mi>d</mi><mo>,</mo><mi>b</mi><mo>,</mo><mi>c</mi></mrow><mo>)</mo></mrow></mrow></math></entry>
