      <entry>E</entry>
      <entry>A</entry>
      <entry>B</entry>
      <entry>A</entry>
      <entry>A</entry>
      <entry>C</entry>
      <entry>A</entry>
      </row>
      <row>
      <entry>E</entry>
      <entry>A</entry>
      <entry>C</entry>
      <entry>A</entry>
