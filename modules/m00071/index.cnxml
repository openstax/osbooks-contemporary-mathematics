    <math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mn>1</mn><mo rspace="0.28em">⁢</mo><mi>cm</mi><mo>=</mo><mn>10</mn><mo rspace="0.28em">⁢</mo><mi>mm</mi></mrow></math><newline xmlns="http://cnx.rice.edu/cnxml"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mn>10</mn><mo rspace="0.28em">⁢</mo><mi>mm</mi><mo>×</mo><mn>10</mn><mo rspace="0.28em">⁢</mo><mi>mm</mi><mo>×</mo><mn>10</mn><mo rspace="0.28em">⁢</mo><mi>mm</mi><mo>=</mo><mn>1,000</mn><mo rspace="0.28em">⁢</mo><msup><mrow><mi>mm</mi></mrow><mn>3</mn></msup></mrow></math></entry>
    <entry xmlns="http://cnx.rice.edu/cnxml">1 cm<sup>3</sup> = 1,000 mm<sup>3</sup></entry>
    </row>
