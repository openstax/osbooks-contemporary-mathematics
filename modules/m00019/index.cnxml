      <entry xmlns="http://cnx.rice.edu/cnxml"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><mrow><mi>p</mi><mo>∨</mo><mi>q</mi></mrow></math></entry>
      </row>
    </thead>
    <tbody xmlns="http://cnx.rice.edu/cnxml">
    <row xmlns="http://cnx.rice.edu/cnxml">
      <entry>T</entry>
      <entry>T</entry>
      <entry>T</entry>
      <entry/>
      <entry>T</entry>
      <entry>T</entry>
      <entry>T</entry>
      </row>
      <row>
      <entry>T</entry>
      <entry>F</entry>
      <entry>F</entry>
      <entry/>
      <entry>T</entry>
      <entry>F</entry>
      <entry>T</entry>
      </row>
      <row>
      <entry>F</entry>
      <entry>T</entry>
      <entry>F</entry>
      <entry/>
      <entry>F</entry>
