      <section id="sect-00005" class="check-understanding">
      <title>Check Your Understanding</title>
      <exercise id="exer-00013">
      <problem id="prob-00013">
      <para id="para-00039"><link class="os-embed" url="#exercise/CM-C02-M3-Q007"/></para>
      </problem>
      </exercise>
      <exercise id="exer-00014">
      <problem id="prob-00014">
      <para id="para-00040"><link class="os-embed" url="#exercise/CM-C02-M3-Q008"/></para>
      </problem>
      </exercise>
      <exercise id="exer-00015">
      <problem id="prob-00015">
