  <title>Expected Value</title>
  <metadata xmlns:md="http://cnx.rice.edu/mdml">
    <md:title>Expected Value</md:title>
    <md:content-id>m00097</md:content-id>
    <md:uuid>3d4e8c5e-da3e-464c-a5a0-308d03d1c239</md:uuid>
